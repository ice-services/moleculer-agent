{
  "name": "moleculer-agent",
  "version": "0.0.0",
  "description": "Agent to control your Moleculer nodes remotely",
  "main": "index.js",
  "scripts": {
    "ci": "jest --watch",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "demo": "node examples/index.js",
    "deps": "npm-check -u",
    "dev": "nodemon --debug dev/index.js",
    "lint": "eslint --ext=.js src test",
    "lint:fix": "eslint --fix --ext=.js src test",
    "test": "jest --coverage --no-cache"
  },
  "keywords": [
    "microservice",
    "agent",
    "services",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-services/moleculer-agent.git"
  },
  "author": "Ice Services",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "2.13.1",
    "eslint": "4.7.2",
    "jest": "21.2.0",
    "jest-cli": "21.2.0",
    "moleculer": "0.11.0",
    "moleculer-repl": "0.3.1",
    "nats": "0.7.20",
    "nodemon": "1.12.1",
    "npm-check": "5.4.5"
  },
  "peerDependencies": {
    "moleculer": ">= 0.10.0"
  },
  "dependencies": {
    "glob": "7.1.2",
    "lodash": "4.17.4"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/",
      "/test/unit/utils.js"
    ],
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ]
  }
}
